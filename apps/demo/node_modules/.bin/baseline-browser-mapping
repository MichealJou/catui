#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/program/code/code_catui/catui/apps/demo/node_modules/baseline-browser-mapping/dist/node_modules:/Users/program/code/code_catui/catui/apps/demo/node_modules/baseline-browser-mapping/node_modules:/Users/program/code/code_catui/catui/apps/demo/node_modules:/Users/program/code/code_catui/catui/apps/node_modules:/Users/program/code/code_catui/catui/node_modules:/Users/program/code/code_catui/node_modules:/Users/program/code/node_modules:/Users/program/node_modules:/Users/node_modules:/node_modules:/Users/program/code/code_catui/catui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/program/code/code_catui/catui/apps/demo/node_modules/baseline-browser-mapping/dist/node_modules:/Users/program/code/code_catui/catui/apps/demo/node_modules/baseline-browser-mapping/node_modules:/Users/program/code/code_catui/catui/apps/demo/node_modules:/Users/program/code/code_catui/catui/apps/node_modules:/Users/program/code/code_catui/catui/node_modules:/Users/program/code/code_catui/node_modules:/Users/program/code/node_modules:/Users/program/node_modules:/Users/node_modules:/node_modules:/Users/program/code/code_catui/catui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../baseline-browser-mapping/dist/cli.js" "$@"
else
  exec node  "$basedir/../baseline-browser-mapping/dist/cli.js" "$@"
fi
